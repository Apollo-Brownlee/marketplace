{
  "rewrites": [{
    "source": "/",
    "destination": "/api/index"
  },
  {
    "source": "/featured",
    "destination": "/api/featured"
  },
  {
    "source": "/item/:category/:item*",
    "destination": "/api/item?category=:category&item=:item*"
  },
  {
    "source": "/items/:category",
    "destination": "/api/items?category=:category"
  },
  { 
    "source": "/(.*)", 
    "destination": "/api/404" 
  }]
}
